package com.example.problemsolvinglevel01_kotlin

fun main()
{
    var number1:Int = readIntNumber("Please enter number 01: ")
    var number2:Int = readIntNumber("Please enter number 02: ")
    var number3:Int = readIntNumber("Please enter number 03: ")

    print("The max number is: ${getMaxNumber2(number1, number2, number3)}")

}
